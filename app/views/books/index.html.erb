<h1>
  <b>Books</b>
</h1>

<h1>投稿一覧</h1>
<% @books.each do |book| %>
  <p>タイトル</p>
  <span><%= book.title %></span>
<% end %>

<h2>New book</h2>

<%= form_with model:@book, local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'create book' %>
<% end %>
